{
  "name": "internet-banking-authentication",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:single-spa:internetBankingAuthentication",
    "start-zn": "npm run serve-zn:single-spa:internetBankingAuthentication",
    "build": "npm run build:single-spa:internetBankingAuthentication",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "extractLinguals": "ng extract-i18n --output-path src/locale",
    "build:single-spa:internetBankingAuthentication": "node remove-dist.js && ng run internetBankingAuthentication:build-en && ng run internetBankingAuthentication:build-zn",
    "serve:single-spa:internetBankingAuthentication": "ng s --project internetBankingAuthentication --disable-host-check --port 9003 --live-reload false",
    "serve-zn:single-spa:internetBankingAuthentication": "ng s --project internetBankingAuthentication --disable-host-check --port 9003 --live-reload false --configuration development-zn"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@auth0/angular-jwt": "^5.0.2",
    "rxjs": "~6.6.0",
    "single-spa": ">=4.0.0",
    "single-spa-angular": "^5.0.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "12.1.3",
    "@angular-devkit/build-angular": "~12.2.16",
    "@angular/cli": "~12.2.16",
    "@angular/compiler-cli": "~12.2.0",
    "@angular/localize": "^12.2.16",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
}
